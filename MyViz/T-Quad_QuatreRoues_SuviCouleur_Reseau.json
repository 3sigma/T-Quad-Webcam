					"type": "slider",
					"settings": {
						"datasourcename": "ws",
						"variable": "datasources[\"ws\"][\"Robjet\"]",
						"initialvalue": 3.5,
						"min": 0,
						"max": 10,
						"resolution": 1,
						"resetvalue": 3.5,
						"resetcaption": "Réinitialisation"
					}
				}
			]
		}
	],
	"datasources": [
		{
			"name": "ws",
			"type": "websocket",
			"settings": {
				"host": "ws://192.168.0.199:9090/ws",
				"variables_to_send": "couleur,Robjet",
				"refresh_rate": "500",
				"checksum": "none"
			}
		}
	],
	"columns": 3
}