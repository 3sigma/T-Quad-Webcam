						"time_window": 10,
						"value": [
							"datasources[\"ws\"][\"rayon\"]"
						],
						"y_axis_min_range": "0,10",
						"height": 3,
						"pausable": true
					}
				},
				{
					"type": "plot",
					"settings": {
						"title": "Distance à l'objet (cm)",
						"xaxis": "datasource_column",
						"time": "datasources[\"ws\"][\"Temps\"]",
						"time_window": 10,
						"value": [
							"datasources[\"ws\"][\"distance\"]"
						],
						"y_axis_min_range": "0,1",
						"height": 3,
						"pausable": true
					}
				}
			]
