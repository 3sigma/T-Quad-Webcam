				{
					"type": "plot",
					"settings": {
						"title": "Ecart angulaire (degrés)",
						"xaxis": "datasource_column",
						"time": "datasources[\"ws\"][\"Temps\"]",
						"time_window": 10,
						"value": [
							"datasources[\"ws\"][\"ecart_angulaire\"]"
