				"4": 1
			},
			"col_width": "1",
			"widgets": [
				{
					"type": "textarea",
					"settings": {
						"nblines_window": "10",
						"value": [
							"datasources[\"ws\"][\"Raw\"]"
						]
					}
				}
			]
		},
		{
			"title": "Télémétrie",
			"width": 1,
			"row": {
				"3": 13
			},
			"col": {
				"3": 3
